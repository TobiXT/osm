<T3DataStructure>
    <meta>
        <langDisable>1</langDisable>
    </meta>
    <sheets>
        <main>
            <ROOT>
                <TCEforms>
                    <sheetTitle>Lux</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <settings.address>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>LLL:EXT:osm/Resources/Private/Language/locallang_db.xlf:pi1.settings.address</label>
                            <config>
                                <type>input</type>
                                <placeholder>Geschwister-Scholl-Platz, 72074 Tübingen, Deutschland</placeholder>
                                <eval>trim,required</eval>
                            </config>
                        </TCEforms>
                    </settings.address>
                    <settings.zoom>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>LLL:EXT:osm/Resources/Private/Language/locallang_db.xlf:pi1.settings.zoom</label>
                            <config>
                                <type>input</type>
                                <size>10</size>
                                <eval>trim,int</eval>
                                <range>
                                    <lower>0</lower>
                                    <upper>18</upper>
                                </range>
                                <default>14</default>
                                <slider>
                                    <step>1</step>
                                    <width>350</width>
                                </slider>
                            </config>
                        </TCEforms>
                    </settings.zoom>
                    <settings.markertitle>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>LLL:EXT:osm/Resources/Private/Language/locallang_db.xlf:pi1.settings.markertitle</label>
                            <config>
                                <type>input</type>
                                <placeholder>Universität Tübingen</placeholder>
                                <eval>trim</eval>
                            </config>
                        </TCEforms>
                    </settings.markertitle>
                    <settings.markerdescription>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>LLL:EXT:osm/Resources/Private/Language/locallang_db.xlf:pi1.settings.markertitle</label>
                            <config>
                                <type>text</type>
                                <placeholder>1477 gegründete Bildungseinrichtung mit 7 Fakultäten, ca. 28.500 Studenten und 17 Krankenhäusern</placeholder>
                                <eval>trim</eval>
                            </config>
                        </TCEforms>
                    </settings.markerdescription>
                </el>
            </ROOT>
        </main>
    </sheets>
</T3DataStructure>
