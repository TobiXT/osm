{
	"name": "in2code/osm",
	"description": "OSM - OpenStreetMap extension for TYPO3",
	"keywords": [
		"typo3",
		"osm",
		"OpenStreetMap"
	],
	"homepage": "https://github.com/in2code-de/osm",
	"authors": [
		{
			"name": "Alex Kellner",
			"email": "alexander.kellner@in2code.de",
			"role": "Technical owner",
			"homepage": "https://www.in2code.de"
		}
	],
	"type": "typo3-cms-extension",
	"license": "GPL-2.0-or-later",
	"require": {
		"php": "^7.4 || ^8.0",
		"typo3/cms-core": "^11.5 || ^12.3",
		"ext-json": "*"
	},
	"replace": {
		"typo3-ter/osm": "self.version"
	},
	"suggest": {
		"friendsoftypo3/tt-address": "Show address markers from tt_address records in a map"
	},
	"autoload": {
		"psr-4": {
			"In2code\\Osm\\": "Classes/"
		}
	},
	"extra": {
		"typo3/cms": {
			"extension-key": "osm"
		}
	}
}
